    <owl:Class rdf:about="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#Disorientation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaSymptom"/>
        <rdfs:comment>Feeling confused about time, place, or people.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/allen/ontologies/2023/8/dementia#FolateDeficiency -->

