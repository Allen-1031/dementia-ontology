            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaTreatment -->

    <owl:Class rdf:about="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaTreatment"/>
    


    <!-- http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DifficultyPerformingTasks -->

    <owl:Class rdf:about="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DifficultyPerformingTasks">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaSymptom"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia/isStage"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia/EarlyStage"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Struggling with everyday tasks, such as cooking or getting dressed.</rdfs:comment>
    </owl:Class>
    


