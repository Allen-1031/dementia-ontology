    <!-- http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaDiagnosis -->

    <owl:Class rdf:about="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaDiagnosis">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia/consistsOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#MedicalAssessment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia/isTreatedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaTreatment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaPrevention -->

    <owl:Class rdf:about="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaPrevention">
        <rdfs:comment>These preventive measures can reduce the risk of dementia, but, they do not guarantee complete prevention. It&apos;s always advisable to consult with healthcare professionals for personalized advice and guidance.</rdfs:comment>
    </owl:Class>
    


