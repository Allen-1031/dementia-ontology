    <!-- http://www.semanticweb.org/allen/ontologies/2023/8/dementia#CommunityCare -->

    <owl:Class rdf:about="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#CommunityCare">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaCare"/>
        <rdfs:comment>Community care for dementia patients involves support from home care nurses, who provide medical care and nursing care. It also includes community-based services, social support, and interventions to support caregivers.</rdfs:comment>
    </owl:Class>
    


