    </owl:Class>
    


    <!-- http://www.semanticweb.org/allen/ontologies/2023/8/dementia#Heart-HealthyDiet -->

    <owl:Class rdf:about="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#Heart-HealthyDiet">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaPrevention"/>
        <rdfs:comment>These diets emphasize fruits, vegetables, whole grains, lean proteins, and healthy fats.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/allen/ontologies/2023/8/dementia#HomeCare -->

    <owl:Class rdf:about="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#HomeCare">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/allen/ontologies/2023/8/dementia#DementiaCare"/>
        <rdfs:comment>Home care for dementia patients involves support from family caregivers and the provision of personal control, decision-making, and environmental texture. It follows the principles of comfort, attachments, inclusion, occupation, and identity.</rdfs:comment>
    </owl:Class>
    


